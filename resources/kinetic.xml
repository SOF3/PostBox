<?xml version="1.0" encoding="UTF-8" ?>
<kinetic namespace="PostBox" xmlns="https://rawgit.com/SOF3/libkinetic/master/libkinetic.xsd">
    <series id="group-by-type">
        <command name="postbox">
            <alias name="pb"/>
        </command>

        <onStart>
            <or onFalse="exit">
                <permission name="postbox" not="true"/>
                <permission name="example" not="true"/>
            </or>
        </onStart>

        <list/>
    </series>
</kinetic>
